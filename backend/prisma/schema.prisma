generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model book {
  id   String @id
  name String
}

model book_sample {
  id   String  @id @db.VarChar(255)
  name String? @db.VarChar(255)
}
